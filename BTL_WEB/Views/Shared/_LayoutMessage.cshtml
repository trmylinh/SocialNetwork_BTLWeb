	function PostANewMessage(){
		URL = 'https://localhost:7070/api/messageAPI/messages/';
		var time = new Date($.now());
		var senderId = parseInt(@userId);
		var groupId = +$('#groupId').val();
		var message = $.trim($('#messageContent').val());
		if(message == ""){
			$('#messageContent').focus();
			return;
		}
		
		$.ajax({
			type: 'POST',
			url: URL,
			dataType: 'text',
			contentType: 'application/json',
			data: JSON.stringify({
				"SenderId": senderId,
				"GroupId": groupId,
				"Message1": message,
				"SentDatetime": time,
			}),
			success: function (res) {
				console.log(time, senderId, groupId, message);
				str = `<li class="me" id="message">
										<figure>
											<p id="userName${senderId}" style="margin: 0;">me</p>
											<img src="../../Anh/${res}" alt="">
										</figure>
										<div class="text-box">
												<p>${message}</p>
												<span><i class="ti-check"></i><i class="ti-check"></i> ${time.toISOString().slice(0, -5)}</span>
										</div>
									  </li>`;
					$('#conversations').append(str);
				$('#messageContent').val("");
				console.log(res);
			},
			error: function(res){
				console.log('err');
			}
		});
	}
